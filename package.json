{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@rollup/plugin-dsv": "^2.0.1",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-consts": "^1.0.2",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "array-flat-polyfill": "^1.0.1",
    "bulma": "^0.8.0",
    "clipboard-polyfill": "^2.8.6",
    "rollup-plugin-postcss": "^2.0.3",
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "build-checklatey": "rollup -c --environment PACKAGE_MANAGER:chocolatey",
    "dev-checklatey": "rollup -c -w --environment PACKAGE_MANAGER:chocolatey",
    "build-winget": "rollup -c --environment PACKAGE_MANAGER:winget",
    "dev-winget": "rollup -c -w --environment PACKAGE_MANAGER:winget",
    "build-scoop": "rollup -c --environment PACKAGE_MANAGER:scoop",
    "dev-scoop": "rollup -c -w --environment PACKAGE_MANAGER:scoop",
    "start": "sirv public_checklatey --single",
    "start:dev": "sirv public_checklatey --single --dev"
  }
}
